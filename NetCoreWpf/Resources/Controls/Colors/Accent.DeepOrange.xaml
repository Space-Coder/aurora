<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Accent100S" Color="#FF9E80" />
    <SolidColorBrush x:Key="Accent100ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent200S" Color="#FF6E40" />
    <SolidColorBrush x:Key="Accent200ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent400S" Color="#FF3D00" />
    <SolidColorBrush x:Key="Accent400ForegroundS" Color="White" />
    <SolidColorBrush x:Key="Accent700S" Color="#DD2C00" />
    <SolidColorBrush x:Key="Accent700ForegroundS" Color="White" />

    <Color x:Key="Accent100">#FF9E80</Color>
    <Color x:Key="Accent100Foreground">#000000</Color>
    <Color x:Key="Accent200">#FF6E40</Color>
    <Color x:Key="Accent200Foreground">#000000</Color>
    <Color x:Key="Accent400">#FF3D00</Color>
    <Color x:Key="Accent400Foreground">#FFFFFF</Color>
    <Color x:Key="Accent700">#DD2C00</Color>
    <Color x:Key="Accent700Foreground">#FFFFFF</Color>
</ResourceDictionary>