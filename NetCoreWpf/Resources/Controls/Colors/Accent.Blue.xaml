<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Accent100S" Color="#82B1FF" />
    <SolidColorBrush x:Key="Accent100ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent200S" Color="#448AFF" />
    <SolidColorBrush x:Key="Accent200ForegroundS" Color="White" />
    <SolidColorBrush x:Key="Accent400S" Color="#2979FF" />
    <SolidColorBrush x:Key="Accent400ForegroundS" Color="White" />
    <SolidColorBrush x:Key="Accent700S" Color="#2962FF" />
    <SolidColorBrush x:Key="Accent700ForegroundS" Color="White" />

    <Color x:Key="Accent100">#82B1FF</Color>
    <Color x:Key="Accent100Foreground">#000000</Color>
    <Color x:Key="Accent200">#448AFF</Color>
    <Color x:Key="Accent200Foreground">#FFFFFF</Color>
    <Color x:Key="Accent400">#2979FF</Color>
    <Color x:Key="Accent400Foreground">#FFFFFF</Color>
    <Color x:Key="Accent700">#2962FF</Color>
    <Color x:Key="Accent700Foreground">#FFFFFF</Color>
</ResourceDictionary>