<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Accent100S" Color="#80D8FF" />
    <SolidColorBrush x:Key="Accent100ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent200S" Color="#40C4FF" />
    <SolidColorBrush x:Key="Accent200ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent400S" Color="#00B0FF" />
    <SolidColorBrush x:Key="Accent400ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent700S" Color="#0091EA" />
    <SolidColorBrush x:Key="Accent700ForegroundS" Color="White" />

    <Color x:Key="Accent100">#80D8FF</Color>
    <Color x:Key="Accent100Foreground">#000000</Color>
    <Color x:Key="Accent200">#40C4FF</Color>
    <Color x:Key="Accent200Foreground">#000000</Color>
    <Color x:Key="Accent400">#00B0FF</Color>
    <Color x:Key="Accent400Foreground">#000000</Color>
    <Color x:Key="Accent700">#0091EA</Color>
    <Color x:Key="Accent700Foreground">#FFFFFF</Color>
</ResourceDictionary>