<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Accent100S" Color="#FFE57F" />
    <SolidColorBrush x:Key="Accent100ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent200S" Color="#FFD740" />
    <SolidColorBrush x:Key="Accent200ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent400S" Color="#FFC400" />
    <SolidColorBrush x:Key="Accent400ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent700S" Color="#FFAB00" />
    <SolidColorBrush x:Key="Accent700ForegroundS" Color="Black" />

    <Color x:Key="Accent100">#FFE57F</Color>
    <Color x:Key="Accent100Foreground">#000000</Color>
    <Color x:Key="Accent200">#FFD740</Color>
    <Color x:Key="Accent200Foreground">#000000</Color>
    <Color x:Key="Accent400">#FFC400</Color>
    <Color x:Key="Accent400Foreground">#000000</Color>
    <Color x:Key="Accent700">#FFAB00</Color>
    <Color x:Key="Accent700Foreground">#000000</Color>
</ResourceDictionary>