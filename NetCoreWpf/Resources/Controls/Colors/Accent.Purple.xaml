<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Accent100S" Color="#EA80FC" />
    <SolidColorBrush x:Key="Accent100ForegroundS" Color="Black" />
    <SolidColorBrush x:Key="Accent200S" Color="#E040FB" />
    <SolidColorBrush x:Key="Accent200ForegroundS" Color="White" />
    <SolidColorBrush x:Key="Accent400S" Color="#D500F9" />
    <SolidColorBrush x:Key="Accent400ForegroundS" Color="White" />
    <SolidColorBrush x:Key="Accent700S" Color="#AA00FF" />
    <SolidColorBrush x:Key="Accent700ForegroundS" Color="White" />

    <Color x:Key="Accent100">#EA80FC</Color>
    <Color x:Key="Accent100Foreground">#000000</Color>
    <Color x:Key="Accent200">#E040FB</Color>
    <Color x:Key="Accent200Foreground">#FFFFFF</Color>
    <Color x:Key="Accent400">#D500F9</Color>
    <Color x:Key="Accent400Foreground">#FFFFFF</Color>
    <Color x:Key="Accent700">#AA00FF</Color>
    <Color x:Key="Accent700Foreground">#FFFFFF</Color>

</ResourceDictionary>